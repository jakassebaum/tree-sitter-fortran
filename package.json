{
  "name": "tree-sitter-fortran",
  "version": "0.5.2",
  "description": "Fortran grammar for tree-sitter (Node.js v24+ compatible)",
  "keywords": [
    "parser",
    "fortran",
    "tree-sitter"
  ],
  "homepage": "https://github.com/jakassebaum/tree-sitter-fortran#readme",
  "bugs": {
    "url": "https://github.com/jakassebaum/tree-sitter-fortran/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakassebaum/tree-sitter-fortran.git"
  },
  "license": "MIT",
  "author": "Matt Stadelman <stadelmanma@gmail.com> (https://github.com/stadelmanma)",
  "type": "commonjs",
  "main": "bindings/node",
  "types": "bindings/node",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "scripts": {
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
